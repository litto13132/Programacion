#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;

void main()
{
	float nota1,nota2;
	cout<<"ingrese la primera nota: ";
	cin>>nota1,
	cout<<"ingrese la segunda nota: ";
	cin>>nota2;
	nota2=nota1+nota2;
	cout<<"ingrese la tercera nota: ";
	cin>>nota1;
	nota1=nota1+nota2;
	cout<<"ingrese la cuarta nota: ";
	cin>>nota2;
	nota2=nota1+nota2;
	cout<<"ingrese la quinta nota: ";
	cin>>nota1;
	nota1=(nota1+nota2)/5;
	cout<<"el promedio es: "<<nota1;
	getch();
}

Proceso promedio
	Escribir "ingrese la primera nota"
	Leer nota1
	Escribir "ingrese la segunda nota"
	Leer nota2
	nota2<-nota1+nota2
	Escribir "ingrese la tercera nota"
	Leer nota1
	nota1<-nota1+nota2
	Escribir "ingrese la cuarta nota"
	Leer nota2
	nota2<-nota1+nota2
	Escribir "ingrese la quinta nota"
	Leer nota1
	nota1<-(nota1+nota2)/5
	Escribir "el promedio es ",nota1
FinProceso
