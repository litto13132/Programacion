	cout<<"la variable b es: "<<endl<<b;
	getch();
}

Proceso sin_titulo
	Leer a
	Leer b
	aux<-a
	a<-b
	b<-aux
	Escribir "el valor de a es: ",a
	Escribir "el valor de b es: ",b
FinProceso


